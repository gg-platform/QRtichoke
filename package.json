{
  "name": "react-qr-code-generator",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "description": "Secure React QR Code Generator with advanced features and enterprise-grade security",
  "keywords": ["qr-code", "generator", "react", "typescript", "fluent-ui"],
  "author": "gg-platform",
  "license": "MIT",
  "homepage": "https://gg-platform.github.io/QRtichoke",
  "repository": {
    "type": "git",
    "url": "https://github.com/gg-platform/QRtichoke.git"
  },
  "bugs": {
    "url": "https://github.com/gg-platform/QRtichoke/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm outdated",
    "deploy:preview": "npm run build && npm run preview",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:github": "VITE_BASE_PATH=/QRtichoke/ npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.2.0",
    "@fluentui/react-components": "^9.72.4",
    "@fluentui/react-icons": "^2.0.313",
    "@types/qrcode": "^1.5.6",
    "dompurify": "^3.3.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
